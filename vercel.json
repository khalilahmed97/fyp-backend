{ 
  "version": 2,
 
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node",
      "config": {
        "webpack": true
      }
    }
  ],
  "routes": [
    {
      "src": "./src/index.js",
      "dest": "./src/index.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
  
}